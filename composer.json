    "type": "bolt-extension",
    "keywords": ["livefyre", "comments"],
    "require": {
        "bolt/bolt": ">=2.0.0,<3.0.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Corentin Mors",
